{
  "input": {
    "input_type": "kafka",
    "options": {
      "server": "176.120.25.195",
      "port": 29092,
      "topic": "testtag01",
      "batchDuration": 10,
      "sep": ","
    }
  },
  "output": {
    "method": "stdout",
    "options": {
    }
  },
  "processing": {
    "transformation": "packet_size;traffic: mult(packet_size,sampling_rate)",
    "aggregations": {
      "operation_type": "reduce",
      "rule": " Max(packet_size), Max(traffic)"
	}
  },
  "databases": {
    "country": "./GeoLite2-Country.mmdb",
    "city": "./GeoLite2-City.mmdb",
    "asn": "./GeoLite2-ASN.mmdb"
  }
}